{
  "name": "radapter",
  "version": "0.3.2",
  "description": "React component adapter for legacy Angularjs apps",
  "main": "./dist/main.js",
  "author": "Daniel Del Core",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && webpack",
    "build:watch": "npm run build -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint index.js src",
    "preversion": "npm run lint && npm run test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danieldelcore/radapter.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "React",
    "Angular",
    "adapter",
    "component",
    "directive"
  ],
  "bugs": {
    "url": "https://github.com/danieldelcore/radapter/issues"
  },
  "homepage": "https://github.com/danieldelcore/radapter#readme",
  "devDependencies": {
    "angular": "^1.5.11",
    "angular-mocks": "1.5.11",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.4.3",
    "ng-annotate-loader": "^0.6.1",
    "prop-types": "^15.6.2",
    "react": "^16",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.3.2",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3"
  },
  "peerDependencies": {
    "angular": ">=1.5.6",
    "react": ">=16",
    "react-dom": ">=16.4.1"
  }
}
